{
  "name": "brinkbit.js",
  "version": "0.1.2",
  "description": "Javascript Brinkbit SDK",
  "main": "dist/brinkbit.js",
  "scripts": {
    "start": "./node_modules/.bin/pm2 start test/mockServer/index.js --name='mockServer'",
    "stop": "./node_modules/.bin/pm2 stop mockServer",
    "test": "yarn start; cross-env KARMA_REPORTER=mocha NODE_ENV=test karma start; yarn stop;",
    "karma": "cross-env KARMA_REPORTER=json NODE_ENV=test karma start",
    "build": "npm run build:dev; npm run build:minify",
    "build:dev": "cross-env NODE_ENV=build ./node_modules/.bin/webpack --progress --debug --profile",
    "watch": "./node_modules/.bin/webpack --progress --colors --watch",
    "build:minify": "node ./scripts/minify.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brinkbit/brinkbit.js.git"
  },
  "keywords": [
    "SDK",
    "Brinkbit",
    "javascript",
    "js",
    "game",
    "baas",
    "client"
  ],
  "author": "Brinkbit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brinkbit/brinkbit.js/issues"
  },
  "homepage": "https://github.com/Brinkbit/brinkbit.js#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "brinkbit": "^0.1.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-brinkbit": "^0.1.3",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "express-bearer-token": "^2.1.0",
    "fs-extra": "^3.0.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-json-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "shelljs": "^0.7.8",
    "uglify-js": "^3.0.18",
    "uuid": "^3.1.0",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "browser-request": "^0.3.3",
    "custom-error-instance": "^2.1.1",
    "event-emitter": "^0.3.5",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "pm2": "^2.6.1",
    "resolve-url": "^0.2.1",
    "store": "^2.0.12",
    "validate.js": "^0.11.1"
  }
}
